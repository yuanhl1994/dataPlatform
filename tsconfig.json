{
    "compilerOptions": {
      "sourceMap": true,
      "noImplicitAny": false,
      "module": "ESNext",
      "moduleResolution": "node",
      "target": "ESNext",
      "jsx": "react",
      "esModuleInterop": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/containers/*"],
        "#/*": ["src/components/*"],
        "$/*": ["src/services/*"],
        "%/*": ["src/utils/*"],
      }
    },
    "include": [
      "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}